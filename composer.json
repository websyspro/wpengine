{
  "name": "websyspro/wp-engine",
  "description": "WordPress runtime installer for Composer-based projects",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Websyspro\\WpEngine\\": "src/"
    }
  },
  "bin": [
    "bin/install.php"
  ],
  "extra": {
    "wp-engine": {
      "wordpress": "6.9",
      "locale": "pt_BR"
    }
  },
  "require-dev": {
    "websyspro/commons": "dev-main"
  }
}